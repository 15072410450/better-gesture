    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wensiyuanseven/better-gesture.git"
